{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"tabWidth": 2,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"bracketSpacing": true
			}
		}
	]
}
